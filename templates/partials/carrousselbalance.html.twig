<div class="col-lg-12 my-4">
    <div class="d-flex justify-content-between align-items-center">
        <div>
            <h6 class="text-muted">Welcome to BitChest <strong class="text-secondary w-px-50">{{app.user.firstname}} {{app.user.lastname}}</strong></h6>
        </div>
        <div class="dropdown">
            <a class="btn btn-primary dropdown-toggle d-flex" href="#" role="button" id="dropdownMenuLink" data-bs-toggle="dropdown" aria-expanded="false">
                <i class='menu-icon me-2 bx bxs-wallet' ></i>
                <div>My Wallets</div>
            </a>

            <ul class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                {{form_start(formChooseWallet)}}
                <div class="m-2">
                    {{form_row(formChooseWallet.name)}}
                </div>
                <div class="m-2">
                    {{form_row(formChooseWallet.Change)}}
                </div>
                {{form_end(formChooseWallet)}}
            </ul>
        </div>
    </div>
    <div
    id="carouselExample-cf"
    class="carousel carousel-dark slide carousel-fade"
    data-bs-ride="carousel"
    >
    <ol class="carousel-indicators">
        <li data-bs-target="#carouselExample-cf" data-bs-slide-to="0" class="active"></li>
        <li data-bs-target="#carouselExample-cf" data-bs-slide-to="1"></li>
    </ol>
    <div class="carousel-inner my-2">
        <div class="carousel-item my-2 active">
            {% include 'partials/cardcrypto.html.twig' %}
        </div>
        <div class="carousel-item my-2">
            {% include 'partials/cardcrypto2.html.twig' %}
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExample-cf" role="button" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExample-cf" role="button" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </a>
    </div>
</div>