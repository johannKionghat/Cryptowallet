<div class="card p-4">
    <div id="container" style="width:100%; height:295px;"></div>
    <script src="https://code.highcharts.com/highcharts.js"></script>
   <script>
        document.addEventListener('DOMContentLoaded', function () {
            const marketData = {{ marketChart.prices|json_encode|raw }};

            Highcharts.chart('container', {
                chart: {
                    type: 'line'
                },
                title: {
                    text: 'Bitcoin Price Chart'
                },
                xAxis: {
                    type: 'datetime'
                },
                yAxis: {
                    title: {
                        text: 'Price (EUR)'
                    }
                },
                series: [{
                    name: 'Bitcoin',
                    data: marketData.map(point => [point[0], point[1]])
                }]
            });
        });
    </script>
</div>
</div>
